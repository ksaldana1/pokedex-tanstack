{
  "name": "poke-tanstack",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "vite --port=3001",
    "build": "vite build",
    "serve": "vite preview",
    "start": "vite",
    "gql-schema": "gql.tada generate schema 'http://localhost:3030/graphql' --output './schema.graphql'",
    "generate": "bun run gql-schema && gql.tada generate output"
  },
  "devDependencies": {
    "@tanstack/router-devtools": "^1.58.9",
    "@tanstack/router-plugin": "^1.58.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^5.4.4"
  },
  "dependencies": {
    "@react-spring/web": "^9.7.4",
    "@tanstack/react-router": "^1.58.9",
    "gql.tada": "^1.8.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "urql": "^4.1.0"
  }
}
